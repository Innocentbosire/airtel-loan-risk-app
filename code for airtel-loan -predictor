      "cell_type": "code",
      "source": [
        "# Create simulated new/unseen customer data (not from training set)\n",
        "new_customers = pd.DataFrame([\n",
        "    {\n",
        "        'age': 27,\n",
        "        'months_registered': 22,\n",
        "        'total_spend': 45000,\n",
        "        'txn_count': 10,\n",
        "        'loan_count': 1,\n",
        "        'arrears_days': 10\n",
        "    },\n",
        "    {\n",
        "        'age': 55,\n",
        "        'months_registered': 40,\n",
        "        'total_spend': 300000,\n",
        "        'txn_count': 20,\n",
        "        'loan_count': 0,\n",
        "        'arrears_days': 0\n",
        "    },\n",
        "    {\n",
        "        'age': 37,\n",
        "        'months_registered': 25,\n",
        "        'total_spend': 20000,\n",
        "        'txn_count': 6,\n",
        "        'loan_count': 1,\n",
        "        'arrears_days': 70\n",
        "    },\n",
